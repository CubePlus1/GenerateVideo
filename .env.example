# Video Generation API Configuration
# Copy this file to .env and fill in your values

# API Endpoint (default: local server)
GENVIDEO_API_ENDPOINT=http://127.0.0.1:58778/v1/chat/completions

# API Authentication Token
GENVIDEO_API_TOKEN=your_token_here

# Default Model (optional)
# GENVIDEO_MODEL_NAME=veo_3_1_i2v_s_fast_fl

# Request Timeout in seconds (default: 300)
# GENVIDEO_REQUEST_TIMEOUT=300

# Output Directory (default: ./output/)
# GENVIDEO_DEFAULT_OUTPUT_DIR=./output/
